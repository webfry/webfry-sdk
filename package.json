{
  "name": "webfry-sdk",
  "version": "0.1.0",
  "description": "Official JavaScript/TypeScript SDK for WebFry API",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "npm run build"
  },
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "typescript": "^5.8.2"
  },
  "keywords": [
    "webfry",
    "sdk",
    "security",
    "password",
    "hash"
  ]
}
